<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.gyiscas.smartbidding.ui.PossibleWinBidActivity">
        <com.gyiscas.smartbidding.ui.widget.ToolBarView
            android:id="@+id/toolbarView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"></com.gyiscas.smartbidding.ui.widget.ToolBarView>
    <com.jcodecraeer.xrecyclerview.XRecyclerView
        android:id="@+id/possible_win_bid_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/light_grey"></com.jcodecraeer.xrecyclerview.XRecyclerView>
</LinearLayout>
