<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/light_grey">
    <LinearLayout
        android:background="@drawable/selector_section_info_listview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/userSuggest_ll"
        android:orientation="vertical">
        <TextView
            android:id="@+id/section_name_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/li_28dp_size"
            android:text="@string/dan_wei_ming_cheng"
            android:textColor="@color/dark_grey"
            android:textSize="@dimen/li_14sp_size" />
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/li_16dp_size"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/section_dutyname_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/li_12dp_size"
                android:text="@string/zhi_ban_ren"
                android:textColor="@color/dark_grey"
                android:textSize="@dimen/li_12sp_size" />
            <TextView
                android:id="@+id/section_tel_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/li_12dp_size"
                android:text="@string/0851-6666666"
                android:textColor="@color/dark_grey"
                android:textSize="@dimen/li_12sp_size" />
        </LinearLayout>
        </LinearLayout>
</LinearLayout>