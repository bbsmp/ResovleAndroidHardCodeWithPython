<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <com.gyiscas.smartbidding.ui.widget.ToolBarView
        android:id="@+id/toolbarView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:layout_marginTop="@dimen/li_8dp_size"
        android:id="@+id/contentTv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:text="@string/nei_rong"
        android:textSize="@dimen/li_16sp_size"
        android:textColor="@color/li_content_black"
        android:lineSpacingExtra="@dimen/li_4dp_size"
        android:padding="@dimen/li_8dp_size"
        />
    <TextView
        android:id="@+id/dateTv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="@dimen/li_16sp_size"
        tools:text="@string/nei_rong_888"
        android:textColor="@color/li_content_black"
        android:gravity="center_vertical|right"
        android:padding="@dimen/li_8dp_size"
        />
</LinearLayout>