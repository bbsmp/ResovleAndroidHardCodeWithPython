<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

<FrameLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/content_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.gyiscas.smartbidding.ui.mine.BidFragment">

            <android.support.design.widget.TabLayout
                android:id="@+id/id_tablayout_my"
                android:layout_width="match_parent"
                android:layout_height="@dimen/li_36dp_size"
                android:background="@color/colorWhite"
                app:tabGravity="center"
                app:tabMode="fixed"
                app:tabTextAppearance="@style/tabTextAppearance"
                app:tabBackground="@drawable/selector_tab_bg"
                app:tabIndicatorColor="@color/colorWhite"
                app:tabSelectedTextColor="@color/colorPrimary"
                app:tabTextColor="@color/colorGreyBtn"
                app:tabIndicatorHeight="0dp"
                />
        <android.support.v4.view.ViewPager
            android:id="@+id/id_viewpager_my"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/id_tablayout_my"
            app:layout_behavior="@string/appbar_scrolling_view_behavior" />


    </RelativeLayout>
    <Button
        android:id="@+id/go_btn"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/li_32dp_size"
        android:gravity="center"
        android:layout_gravity="center"
        android:background="@drawable/selector_flat_button"
        android:textColor="@color/colorGreyBtn"
        android:textSize="@dimen/li_15sp_size"
        android:text="请先完善资料"
        android:paddingRight="@dimen/li_16dp_size"
        android:paddingLeft="@dimen/li_16dp_size"
        />
</FrameLayout>
</android.support.design.widget.CoordinatorLayout>